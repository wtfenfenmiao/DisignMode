package observer;

import display.Display;

public class panel3 implements Observer,Display{
	private int sun;
	public void update(int tempreture,int humidity,int sun)
	{
		this.sun=sun;
		display();
	}
	public void display()
	{
		System.out.println("this is panel3!");
		System.out.println("the sun is "+sun);
	}
}